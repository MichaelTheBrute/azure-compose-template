# Local Development Environment Variables
# Copy this file to .env.local and modify as needed

# Database Configuration (already set in docker-compose.local.yml)
# DATABASE_URL=postgresql://postgres:postgres@postgres:5432/sliostudio

# Application Environment
AZURE_ENV_NAME=local

# PostgreSQL Database Settings (for direct database access if needed)
DB_HOST=localhost
DB_USER=postgres
DB_PASS=postgres
DB_NAME=sliostudio
DB_PORT=5432

# Redis Settings
REDIS_HOST=localhost
REDIS_PORT=6379

# Backend API URL (for frontend)
BACKEND_URL=http://localhost:5000

# Development flags
DEBUG=true
FLASK_ENV=development
